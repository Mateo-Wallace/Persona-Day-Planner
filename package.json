{
  "name": "persona-day-planner",
  "version": "1.0.0",
  "description": "Full Stack app that allows users to plan their day. Inspired by the persona series",
  "main": "server/server.js",
  "scripts": {
    "start": "node server/server.js",
    "develop": "concurrently \"cd server && npm run watch\" \"cd client && npm start\"",
    "install": "cd server && npm i && cd ../client && npm i",
    "build": "cd client && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mateo-Wallace/Persona-Day-Planner.git"
  },
  "keywords": [
    "mongodb",
    "react",
    "nodejs",
    "express",
    "persona"
  ],
  "author": "Mateo-Wallace",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Mateo-Wallace/Persona-Day-Planner/issues"
  },
  "homepage": "https://github.com/Mateo-Wallace/Persona-Day-Planner#readme",
  "devDependencies": {
    "concurrently": "^5.1.0"
  },
  "dependencies": {
    "apollo-server-express": "^2.25.3",
    "express": "^4.17.2",
    "graphql": "^15.8.0"
  }
}
